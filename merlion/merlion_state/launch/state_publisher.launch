<launch>
  <arg name="ns" default="merlion_state"/>

  <group ns="$(arg ns)">
    <node name="state_publisher" pkg="merlion_state" type="state_publisher.py" output="screen">
      <remap from="/$(arg ns)/input/imu" to="/$(arg ns)/imu"/>
      <remap from="/$(arg ns)/input/pressure" to="/$(arg ns)/pressure"/>
    </node>
  </group>
</launch>
